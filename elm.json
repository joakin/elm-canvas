{
    "type": "package",
    "name": "linsyking/elm-canvas",
    "summary": "2D drawing API based on DOM Canvas, but nicer",
    "license": "BSD-3-Clause",
    "version": "1.0.0",
    "exposed-modules": {
        "Main module": [
            "Canvas"
        ],
        "Settings": [
            "Canvas.Settings",
            "Canvas.Settings.Text",
            "Canvas.Settings.Line",
            "Canvas.Settings.Advanced"
        ],
        "Textures": [
            "Canvas.Texture"
        ]
    },
    "elm-version": "0.19.0 <= v < 0.20.0",
    "dependencies": {
        "avh4/elm-color": "1.0.0 <= v < 2.0.0",
        "elm/core": "1.0.0 <= v < 2.0.0",
        "elm/html": "1.0.0 <= v < 2.0.0",
        "elm/json": "1.0.0 <= v < 2.0.0"
    },
    "test-dependencies": {}
}
